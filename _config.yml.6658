    value: "https://twitter.com/TestVFW"

exclude:
  - "/vendor/"
  - "Gemfile"
  - "Gemfile.lock"
  - "README.md"
  - "LICENSE.md"
  - "*.gemspec"

keep_files:
  - "CNAME"
  - ".git"
  - ".gitignore"

gems:
  - jekyll-sitemap

# Site settings
title: Test VFW Post
subtitle: "Test VFW Post"
url: "https://testvfw.org" # the base hostname & protocol for your site
# baseurl: "/the/subpath/of/your/site" # Set this value to "" if you want your site to be root
baseurl: ""
newsletter: "https://newslettersignup.com"
cover: "/assets/cover.png"
logo: "logo-example.png"

# Build settings
markdown: kramdown

# Google analytics
ga_tracking_id: "UA-92630193-1"

google:
  tag_manager:
    container_id: GTM-MT9RKR2

# Category descriptions (for archive pages)
descriptions:
  - cat: jekyll
    desc: "Posts describing Jekyll setup techniques."

  - cat: dummy
    desc: "Just some placeholder posts, lorem ipsum and the rest."
